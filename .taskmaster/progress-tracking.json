      "analysis_period": "90_days",
      "comparison_points": ["previous_week", "previous_month", "quarter_start"]
    },
    "bottleneck_detection": {
      "enabled": true,
      "indicators": [
        "task_queue_buildup",
        "agent_response_delays",
        "quality_gate_failures",
        "resource_contention"
      ],
      "auto_suggestions": true
    },
    "optimization_recommendations": {
      "categories": [
        "agent_assignment",
        "task_prioritization",
        "quality_improvement",
        "process_optimization"
      ],
      "confidence_threshold": 0.8,
      "implementation_automation": true
    }
  },
  "data_storage": {
